<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sihvitb.ticklus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ticklus">
        <activity
            android:name=".Home_nav"
            android:label="@string/title_activity_home_nav"
            android:theme="@style/Theme.Ticklus.NoActionBar"
            android:exported="false"/>
        <activity
            android:name=".Registeration_page"
            android:exported="false"
            android:theme="@style/Theme.Noaction" />
        <activity
            android:name=".Login_page"
            android:exported="false"
            android:theme="@style/Theme.Noaction" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Noaction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>